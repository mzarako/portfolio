{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio",
  "main": "index.js",
  "repository": "https://github.com/mzarako/portfolio.git",
  "scripts": {
    "start": "npm run watch:css & npm run start:dev-server",
    "start:dev-server": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:js": "webpack",
    "build:css": "rm -f build/style.css && lessc style/style.less build/style.css",
    "build:html": "rm -f build/index.html && cp index.html build/",
    "build:favicons": "cp -r favicons/* build/",
    "build": "npm run build:html",
    "watch:css": "npm run build:css && chokidar-cmd -c 'npm run build:css' -t style/**/*.less"
  },
  "author": "Monika Zarako-Dobrowner",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chokidar": "^1.6.1",
    "chokidar-cmd": "^1.2.1",
    "less": "^2.7.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.16.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
  }
}
